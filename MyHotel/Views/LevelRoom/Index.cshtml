@model IEnumerable<MyHotel.ViewModel.LevelRoom.LevelRoomViewModel>

@{
    ViewBag.Title = "Index";
}

<!-- Content Header (Page header) -->
<div class="content-header">
    <div class="container-fluid">
        <div class="row mb-2">
            <div class="col-sm-6">
                <h1 class="m-0 text-dark">Pisos</h1>
            </div><!-- /.col -->
            <div class="col-sm-6">
                <ol class="breadcrumb float-sm-right">
                    <li class="breadcrumb-item"><a href="@Url.Action("Index","Home")">Home</a></li>
                    <li class="breadcrumb-item active">Pisos</li>
                </ol>
            </div><!-- /.col -->
        </div><!-- /.row -->
    </div><!-- /.container-fluid -->
</div>
<!-- /.content-header -->


<p>
    <a id="btnNuevo" class="btn btn-primary" href="@Url.Action("Create","LevelRoom")">Nuevo</a>
</p>

<div class="card">
    <div class="card-header">
        <h3 class="card-title"></h3>
    </div>
    <!-- /.card-header -->
    <div class="card-body" id="levelcard">
       @Html.Partial("Table",Model)
    </div>
    <!-- /.card-body -->
</div>


<div id="modalContent">

</div>


@section Scripts {
   
    <script src="~/Content/dist/js/Level.js"></script>
}

