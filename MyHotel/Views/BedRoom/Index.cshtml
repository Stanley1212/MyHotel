@model IEnumerable<MyHotel.ViewModel.BedRoom.BedRoomListViewModel>


@{
    ViewBag.Title = "Index";
}

<!-- Content Header (Page header) -->
<div class="content-header">
    <div class="container-fluid">
        <div class="row mb-2">
            <div class="col-sm-6">
                <h1 class="m-0 text-dark">Habitaciones</h1>
            </div><!-- /.col -->
            <div class="col-sm-6">
                <ol class="breadcrumb float-sm-right">
                    <li class="breadcrumb-item"><a href="@Url.Action("Index","Home")">Home</a></li>
                    <li class="breadcrumb-item active">Habitaciones</li>
                </ol>
            </div><!-- /.col -->
        </div><!-- /.row -->
    </div><!-- /.container-fluid -->
</div>
<!-- /.content-header -->


<div class="card">
    <div class="card-header">
        <h3 class="card-title"></h3>
        <a id="btnNuevo" class="btn btn-primary" href="@Url.Action("Create","BedRoom")">Nuevo</a>
    </div>
    <!-- /.card-header -->
    <div class="card-body" id="card">
        @Html.Partial("_Table", Model)
    </div>
    <!-- /.card-body -->
</div>


<div id="modalContent">

</div>

@section Scripts {
    <script src="~/Content/dist/js/BedRoom.js"></script>
}


